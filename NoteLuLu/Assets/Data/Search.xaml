<Window x:Class="NoteLuLu.Assets.Data.Search"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoteLuLu.Assets.Data"
        mc:Ignorable="d"
        Title="Search" Height="150" Width="250" MaxHeight="150" MaxWidth="250" MinHeight="150" MinWidth="250">
    <Grid Background="#FFD4CECE">
        <TextBox x:Name="TbSearch" HorizontalAlignment="Center" Margin="0,20,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="230" FontFamily="Cambria" Foreground="#FF363333" Height="28" FontSize="16" Background="#FFB4B0B0"/>
        <Button x:Name="BtnSearch" Content="Search" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Width="98" FontFamily="Cambria" FontSize="16" Foreground="#FF363333" Height="28" Background="#FFB4B0B0" Click="BtnSearch_Click"/>

    </Grid>
</Window>